// 查询媒体属性，仅对RTSP Client输入虚拟子卡
{
    "jsonrpc": "2.0",
    "method": "media",
    "param": null,
    "id": 1
}

// 回复查询媒体属性
{
    "jsonrpc": "2.0",
    "result": {
        "enc": {
            "vcodec": "h.264",
            "acodec": "g.711a",
            "vpt": 96,
            "apt": 8
        }
    },
    "id": 1
}

// 设置转码
{
    "jsonrpc": "2.0",
    "method": "trans.set",
    "params": {
        "chan": 1,
        "input": {
            "ctrl": 1,
            "cast": 1,
            // 上游转发子卡的IP地址, 可以是rtsp地址（对于RTSP Client）
            // card://<上游IP>: <转发服务链接port>/<转发服务输入port>
            "url": "card://192.168.1.10:88/6000_60002"
        },
        "dec": {
            "vcodec": "h.264",
            "acodec": "g.711a",
            "vpt": 96,
            "apt": 8
        },
        "enc": {
            "vcodec": "h.265",
            "acodec": "g.711a",
            "vpt": 98,
            "apt": 8
            },
        "output": {
            "ctrl": 1,
            "ip": "192.168.1.20",
            "vport": "6000",
            "aport": "6002"
        }
    },
    "id": 1
}

// 回复
{
    "jsonrpc": "2.0",
    "result": "ok",
    "id"
}